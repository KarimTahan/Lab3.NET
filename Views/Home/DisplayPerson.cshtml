
@{
    ViewData["Title"] = "DisplayPerson";
}

<h1>Display Person</h1>

@if (Model != null)
{
    <table>
        <tr>
            <td>First Name:</td>
            <td>@Model.FirstName</td>
        </tr>
        <tr>
            <td>Last Name:</td>
            <td>@Model.LastName</td>
        </tr>
        <tr>
            <td>Age:</td>
            <td>@Model.Age</td>
        </tr>
        <tr>
            <td>Email Address:</td>
            <td>@Model.EmailAddress</td>
        </tr>
        <tr>
            <td>Date of Birth:</td>
            <td>@Model.DateofBirth</td>
        </tr>
        <tr>
            <td>Password:</td>
            <td>@Model.Password</td>
        </tr>
        <tr>
            <td>Description:</td>
            <td>@Model.Description</td>
        </tr>
    </table>

    <br />
    <br />
    <br />
    <a href="/Home/Index">Go Home</a>

}